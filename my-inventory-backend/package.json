{
  "name": "my-inventory-backend",
  "version": "0.1.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "node-ts": "node -r tsconfig-paths/register -r ts-node/register/transpile-only",
    "start": "node ./dist/index.js",
    "start:local": "yarn node-ts ./src",
    "start:dev": "nodemon",
    "test": "jest --forceExit",
    "lint": "eslint src/ --ext .js,.ts",
    "clean": "rm -rf dist",
    "build": "tsc -p ./tsconfig.json",
    "build-all": "yarn clean && yarn build",
    "kill-port": "yarn node-ts src/scripts/kill-port.ts",
    "docs": "typedoc --entryPoints src/index.ts",
    "migrate": "prisma migrate dev",
    "migrate:push": "prisma db push",
    "migrate:reset": "prisma migrate reset --force",
    "env:test": "dotenv -e .env.test -- yarn ",
    "env:ci": "dotenv -e .env.ci -- yarn "
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "dotenv-cli": "^5.0.0",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^27.5.1",
    "kill-port": "^1.6.1",
    "my-inventory-common": "*",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "prisma": "^3.10.0",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.12.0",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@prisma/client": "3.10.0",
    "@quixo3/prisma-session-store": "^3.1.3",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/method-override": "^0.0.32",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.9.3",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.2",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.2.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "method-override": "^3.0.0",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "zod": "^3.11.6"
  }
}
